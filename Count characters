#include <bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cin >> s;

    int freq[256] = {0};
    for(char c : s){
        freq[(int)c]++;
    }

    for(int i = 'A'; i <= 'Z'; i++){
        if(freq[i] > 0){
            cout << (char)i << " " << freq[i] << "\n";
        }
    }

    for(int i = 'a'; i <= 'z'; i++){
        if(freq[i] > 0){
            cout << (char)i << " " << freq[i] << "\n";
        }
    }

    return 0;
}

